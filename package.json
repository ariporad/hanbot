{
	"name": "hanbot",
	"version": "1.4.1",
	"description": "A bot for the Olin Class of 2024",
	"main": "dist/index.js",
	"repository": "https://github.com/ariporad/hanbot",
	"author": "Ari Porad <ari@ariporad.com>",
	"license": "MIT",
	"private": true,
	"scripts": {
		"prestart": "yarn build",
		"start": "node ./dist/index.js",
		"build": "tsc",
		"watch": "tsc --watch",
		"clean": "rm -rf dist",
		"predev": "yarn clean && yarn build",
		"dev": "concurrently -c bgBlue,bgGreen.black -k -n tsc,bot \"tsc --watch\" \"nodemon -w dist dist/index.js\"",
		"travis": "yarn clean && yarn build"
	},
	"devDependencies": {
		"@types/express": "^4.17.6",
		"@types/jsonwebtoken": "^8.5.0",
		"@types/node": "^14.0.1",
		"@types/request-promise": "^4.1.46",
		"concurrently": "^5.2.0",
		"nodemon": "^2.0.4",
		"request": "^2.88.2",
		"typescript": "^3.9.2"
	},
	"dependencies": {
		"discord.js": "^12.2.0",
		"express": "^4.17.1",
		"jsonwebtoken": "^8.5.1",
		"request-promise": "^4.2.5"
	}
}
